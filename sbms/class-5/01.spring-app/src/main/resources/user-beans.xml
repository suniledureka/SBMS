<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
						
	<!-- value as an attribute -->
	<bean id="user1" class="co.edureka.spring.core.beans.User">
		<property name="firstName" value="Pankaj"/>
		<property name="lastName" value="Kumar"/>
	</bean>
	
	<!-- value as an element -->
	<bean id="user2" class="co.edureka.spring.core.beans.User">
		<property name="firstName"> 
		  <value>Abdul</value>
		</property>  
		<property name="lastName">
		  <value>Mohamed</value>
		</property>  
	</bean>
	
	<!--  value with namespace  -->
	<bean id="user3" class="co.edureka.spring.core.beans.User" p:firstName="Mohammed" p:lastName="Siraj"/>
		
	<bean id="user4" class="co.edureka.spring.core.beans.User">
		<constructor-arg value="Mark"/>
		<constructor-arg value="Taison"/>
	</bean>
</beans>
