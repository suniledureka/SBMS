<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	<context:annotation-config/>
	
	<bean id="bmw" class="co.edureka.spring.core.beans.Car">
		<property name="model" value="B M W"/>
		<property name="cost" value="6500000"/>
	</bean>
	
	<bean id="praveen" class="co.edureka.spring.core.beans.Customer">
		<property name="customerName" value="Praveen Kumar"/>	
	</bean>
	
	<bean id="audi" class="co.edureka.spring.core.beans.Car" primary="true">
		<property name="model" value="Audi"/>
		<property name="cost" value="7585000"/>
	</bean>	
</beans>
