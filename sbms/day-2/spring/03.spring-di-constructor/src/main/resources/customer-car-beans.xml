<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">
						
	<bean id="bmw" class="com.modeln.spring.core.beans.Car">
		<property name="model" value="B M W"/>
		<property name="cost" value="6500000"/>
	</bean>	
	
	<bean id="sanjay" class="com.modeln.spring.core.beans.Customer">
		<constructor-arg value="Sajay Patil"/>
		<constructor-arg ref="bmw"/>
	</bean>			

	<bean id="praveen" class="com.modeln.spring.core.beans.Customer">
		<property name="customerName" value="Praveen Kumar"/>
		<constructor-arg ref="bmw"/>
	</bean>			
</beans>